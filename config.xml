<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="us.homeclub.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>HomeClub Mobile</name>
  <description>
        Homeclub's awesome mobile app.
    </description>
  <author email="support@homeclub.us" href="http://homeclub.us">
      HomeClub
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="android">
    <icon src="icon/android/drawable-ldpi/ic_launcher.png" density="ldpi" />
    <icon src="icon/android/drawable-mdpi/ic_launcher.png" density="mdpi" />
    <icon src="icon/android/drawable-hdpi/ic_launcher.png" density="hdpi" />
    <icon src="icon/android/drawable-xhdpi/ic_launcher.png" density="xhdpi" />
  </platform>
  <platform name="ios">
    <!-- iOS 8.0+ -->
    <!-- iPhone 6 Plus  -->
    <icon src="icon/ios/AppIcon.appiconset/Icon-60@3x.png" width="180" height="180" />
    <!-- iOS 7.0+ -->
    <!-- iPhone / iPod Touch  -->
    <icon src="icon/ios/Icon-60.png" width="60" height="60" />
    <icon src="icon/ios/AppIcon.appiconset/Icon-60@2x.png" width="120" height="120" />
    <!-- iPad -->
    <icon src="icon/ios/AppIcon.appiconset/Icon-76.png" width="76" height="76" />
    <icon src="icon/ios/AppIcon.appiconset/Icon-76@2x.png" width="152" height="152" />
    <!-- iOS 6.1 -->
    <!-- Spotlight Icon -->
    <icon src="icon/ios/AppIcon.appiconset/Icon-40.png" width="40" height="40" />
    <icon src="icon/ios/AppIcon.appiconset/Icon-40@2x.png" width="80" height="80" />
    <!-- iPhone / iPod Touch -->
    <icon src="icon/ios/Icon.png" width="57" height="57" />
    <icon src="icon/ios/Icon@2x.png" width="114" height="114" />
    <!-- iPad -->
    <icon src="icon/ios/Icon-72.png" width="72" height="72" />
    <icon src="icon/ios/Icon-72@2x.png" width="144" height="144" />
    <!-- iPhone Spotlight and Settings Icon -->
    <icon src="icon/ios/AppIcon.appiconset/Icon-Small.png" width="29" height="29" />
    <icon src="icon/ios/AppIcon.appiconset/Icon-Small@2x.png" width="58" height="58" />
    <!-- iPad Spotlight and Settings Icon -->
    <icon src="icon/ios/Icon-50.png" width="50" height="50" />
    <icon src="icon/ios/Icon-50@2x.png" width="100" height="100" />
  </platform>
  <icon src="icon.png" />
  <gap:splash src="splash.png" />
</widget>